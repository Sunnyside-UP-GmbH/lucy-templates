{
  "entryPoints": [
    "*/**/*.ts",
    "*/**/*.tsx"
  ],
  "entryPointStrategy": "expand",
  "out": "docs",
  "plugin": [
    "typedoc-github-theme"
  ],
  "tsconfig": "local.tsconfig.json",
  "excludeExternals": true,
  "externalPattern": [
    "**/node_modules/**",
    "**/.wix/**",
    ".wix/**"
  ],
  "exclude": [
    "./.wix/**/*",
    ".wix/**/*",
    "cypress/**",
    "**/__mocks__/**"
  ],
  "name": "Wix-lucy"
}