{
	"cli": {
		"version": ">= 16.17.0",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"environment": "development",
			"env": {},
			"channel": "development"
		},
		"ios-simulator": {
			"extends": "development",
			"ios": {
				"simulator": true
			},
			"channel": "ios-simulator"
		},
		"preview": {
			"distribution": "internal",
			"environment": "preview",
			"env": {
				"APP_VARIANT": "preview"
			},
			"channel": "preview"
		},
		"production": {
			"autoIncrement": true,
			"environment": "production",
			"env": {},
			"channel": "production"
		}
	},
	"submit": {
		"production": {
			"android": {
				"track": "internal",
				"releaseStatus": "draft"
			},
			"ios": {
				"ascAppId": "6749852709"
			}
		}
	}
}
