  "dependencies": {},
  "scripts": {
    "build": "nx run-many --target=build",
    "dev": "nx run-many --target=dev",
    "lint": "nx run-many --target=lint",
    "format": "prettier --write \"./*.json\" \"**/*.{ts,tsx,md,json,jsonc,json5}\"",
    "check-types": "nx run-many --target=check-types",
    "update": "syncpack -- update",
    "update:list": "syncpack -- list",
    "test": "nx run-many --taget=test",
    "test:projects": "vitest run",
    "test:projects:watch": "vitest --watch",
    "view-report": "nx run-many --target=view-report",
    "test:ui": "vitest --ui",
    "test:e2e": "nx run-many --target=test:e2e"
  },
  "initialized": false,
  "type": "monorepo",
  "packageManager": "pnpm",
  "defaultModulePath": "shared"
}
